<%- include layout/dashboard/header %>
<body>
<%- include layout/dashboard/nav %>
<%- include layout/dashboard/menu %>
  <!-- Page -->
  <div class="page">
    <div class="page-content">
      <div class="panel">
        <div class="panel-body container-fluid">
          <div class="row row-lg">
            <div class="col-lg-12">
              <div class="table-wrap">
                <h4 class="table-title">Currency Exchange Rates</h4>
		<p>NOTE: These figures are placeholder figures and will be updated with live rates soon.</p>
                <div class="table-responsive">
                  <table class="table">
                    <thead>
                      <tr>
                        <th>Currency</th>
                        <th>Token</th>
                        <th>Rate</th>
                      </tr>
                    </thead>
                    <tbody>
                    <% rate_link.forEach(function(link) { %>
                      <tr>
                        <td><%= link.type %></td>
                        <td><%= link.token %></td>
                        <td><b><%= link.rate %></b></td>
                      </tr>
                    <% }); %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
<!--	<% rates.forEach(function(rate) { %>
	  <p><%= rate %></p>
	<% }); %>-->
    </div>
  </div>
  <!-- End Page -->
</body>
<%- include layout/dashboard/footer %>
