<%- include layout/dashboard/header %>
<body>
<%- include layout/dashboard/nav %>
<%- include layout/dashboard/menu %>
  <!-- Page -->
  <div class="page">
    <div class="page-content">
      <div class="panel">
        <div class="panel-body container-fluid">
          <div class="row row-lg">
	    <div class="col-lg-12">
              <div class="table-wrap">
                <h4 class="table-title">Jobs</h4>
           	<% errors.forEach(function(err) { %>
                  <h3><%= err %></h3>
           	<% }); %>
                <div class="table-responsive">
                  <table class="table" id="tx">
                    <thead>
                      <tr>
                        <th>Project</th>
                        <th>Org</th>
                        <th>Currency</th>
                        <th>Amount</th>
                        <th>Ref</th>
                        <th>Description</th>
                        <th>Actions</th>
                      </tr>
                    </thead>
                    <tbody>
           	        <% jobs.forEach(function(job) { %>
                        <tr>
                          <td><%= job.name %></td>
                          <td><%= job.org %></td>
                          <td><%= job.currency %></td>
                          <td><%= job.amount %></td>
                          <td><%= job.ref %></td>
                          <td><%= job.status %></td>
                          <td><a href="/wallet?action=jobs&sub=auth_deposit&ref=<%= job.ref %>">auth deposit</a></td>
                        </tr>
           	        <% }); %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
	</div>
      </div>
    </div>
  </div>
</body>
<%- include layout/dashboard/footer %>
